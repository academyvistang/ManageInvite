@model IencircleAdmin.Models.EventViewModel

@{
    ViewData["Title"] = "Download";
    Layout = "~/Views/Shared/_LayoutGuest.cshtml";
}


<div class="contact-area">
    <div class="contact">
        <main>
            <section>
                <div class="content" style="background-color:@Model.ColourCode;">
                    @*<img src="https://s3-us-west-2.amazonaws.com/s.cdpn.io/256492/_mLIxaKY_400x400.jpg" alt="Profile Image">*@
                    <img src="@Model.BusinessLogo" />
                    <aside>
                        <h1> @Html.DisplayFor(model => model.Fullname)</h1>
                        <p>  @Html.DisplayFor(model => model.ColourCodeDescription)</p>
                        <p>  @Html.DisplayFor(model => model.CompanyName)</p>
                        <p>  @Html.DisplayFor(model => model.Email)</p>
                        <p>  @Html.DisplayFor(model => model.NoOfInvitess) person(s) allowed</p>
                        @if (Model.InviteAcceped.HasValue && Model.InviteAcceped.Value)
                        {
                            <p>Thank you for accepting our invite. Host notified</p>
                        }
                    </aside>

                    <button>

                        @if (Model.InviteAcceped.HasValue && Model.InviteAcceped.Value)
                        {
                            <span style="font-size:12px;">@Model.AppBusinessName</span>
                        }
                        else
                        {
                            <span>Inkheart Studios</span>
                        }


                        <svg xmlns="http://www.w3.org/2000/svg" width="48" height="48" viewBox="0 0 48 48"> <g class="nc-icon-wrapper" fill="#444444"> <path d="M14.83 30.83L24 21.66l9.17 9.17L36 28 24 16 12 28z"></path> </g> </svg>
                    </button>
                </div>

                @*<div class="title"><p>Lauch Fay(Robot)</p></div>*@
            </section>


